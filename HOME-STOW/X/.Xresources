!==========
! Urxvt
!==========
! unicode  
URxvt.imLocale:         en_AU.UTF-8

!URxvt.termName:         rxvt-unicode-256color
URxvt.cutchars:         `"()'*<>[]{|}"

!URxvt.geometry:         100x45
URxvt.borderLess:       false
URxvt.transparent:      false
URxvt.depth:            32
URxvt.background:       rgba:0000/0000/0000/cccc
URxvt.foreground:       white

URxvt.saveLines:         10000
URxvt.xftAntialias:      true
URxvt.hinting:           true
!URxvt.letterSpace:       0
URxvt.skipBuiltinGlyphs: true
#define fonts            xft:Inconsolata for Powerline:pixelsize=16,\
                         xft:DejaVu Sans Mono for Powerline:pixelsize=16,\
                         xft:NotoSansSymbols2-Regular:pixelsize=32,\
                         xft:PowerlineSymbols:pixelsize=32,\
                         xft:Inconsolata Nerd Font Complete,\
                         xft:Noto Emoji Nerd Font Complete,\
                         xft:Source Han Sans Normal,\
                         xft:NotoSansRunic-Regular
URxvt.font: fonts
URxvt.boldFont: fonts
URxvt.italicFont: fonts
URxvt.bolditalicFont: fonts

URxvt*.alt-font-width:  True
URxvt.internalBorder:   0
URxvt.underlineColor:   #999999
URxvt.cursorBlink:      True
URxvt.cursorColor:      #850A0C
URxvt.cursorColor2:     #000000

URxvt.scrollBar:        False
URxvt.scrollBar_right:  False
URxvt.scrollWithBuffer: True
URxvt.scrollTtyKeypress: True
URxvt.urgentOnBell: False

!!black
!*color0:         #151515
!*color8:         #333333
!!red
!*color1:         #AE4747
!*color9:         #EE6363
!!green
!*color2:         #556B2F
!*color10:        #9ACD32
!!brown/yellow
!*color3:         #DAA520
!*color11:        #FFC125
!!blue
!*color4:         #7A7AB0
!*color12:        #4186BE
!!magenta
!*color5:         #8B7B8B
!*color13:        #D8BFD8
!!cyan
!*color6:         #A7A15E
!*color14:        #F0E68C
!!white
!*color7:         #DDDDDD
!*color15:        #FFFFFF

! Tango color palette
URxvt*background:         #262626
URxvt*foreground:         #eeeeec
URxvt*cursorColor:        #606060
! foreground color for underline
URxvt*colorUL:            #8ae234
! line color for underline
URxvt*underlineColor:     #92659a
! black dark/light
URxvt*color0:             #2e3436
URxvt*color8:             #6e706b
! red dark/light
URxvt*color1:             #d54e53
URxvt*color9:             #FF3334
! green dark/light
URxvt*color2:             #4e9a06
URxvt*color10:            #8ae234
! yellow dark/light
URxvt*color3:             #edd400
URxvt*color11:            #fce94f
! blue dark/light
URxvt*color4:             #3465a4
URxvt*color12:            #729fcf
! magenta dark/light
URxvt*color5:             #92659a
URxvt*color13:            #c19fbe
! cyan dark/light
URxvt*color6:             #07c7ca
URxvt*color14:            #63e9e9
! white dark/light
URxvt*color7:             #d3d7cf
URxvt*color15:            #eeeeec
!Color to man pages
URxvt*.colorIT:      #87af5f
URxvt*.colorBD:      #d7d7d7
URxvt*.colorUL: #87afd7

XTerm*metaSendsEscape: true
UXTerm*metaSendsEscape: true
